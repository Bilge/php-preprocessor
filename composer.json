{
    "name": "ircmaxell/php-preprocessor",
    "description": "A PHP Pre-Includ PreProcessor Library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthony Ferrara",
            "email": "ircmaxell@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PhpPreprocessor\\Cli\\": "src/Cli",
            "PhpPreprocessor\\": "lib/"
        }
    },
    "bin": [
        "bin/preprocessor"
    ],
    "require": {
        "php": ">=7.0",
        "cilex/cilex": ">=1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.3",
        "phpunit/php-code-coverage": ">=5.2",
        "symfony/yaml": ">=3.0",
        "nikic/php-parser": ">=3.0"
    },
    "suggest": {
        "symfony/yaml": "Allows parsing of YAML files natively",
        "nikic/php-parser": "Allows pre-processing of PHP files"
    }
}
